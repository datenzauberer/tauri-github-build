{
  "$schema": "https://schema.tauri.app/config/2",
  "productName": "tauri-github-build",
  "version": "0.6.0",
  "identifier": "de.datenzauberer.tauri-github-build",
  "build": {
    "beforeDevCommand": "pnpm dev",
    "devUrl": "http://localhost:1420",
    "beforeBuildCommand": "pnpm build",
    "frontendDist": "../build"
  },
  "app": {
    "windows": [
      {
        "title": "tauri-github-build",
        "width": 800,
        "height": 600
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ],
    "createUpdaterArtifacts": true
  },
  "plugins": {
    "updater": {
      "pubkey": "dW50cnVzdGVkIGNvbW1lbnQ6IG1pbmlzaWduIHB1YmxpYyBrZXk6IDgzMkZCRUYyRkIwNjlDNjIKUldSaW5BYjc4cjR2ZzdaM2hQRkdoYWhVMHArRHZjVFRxTnB6ZEE5em1kWmtaRkdOc0ZCVk5lZGIK",
      "endpoints": [
          "https://github.com/datenzauberer/tauri-github-build/releases/latest/download/latest.json"
      ]
    }
  }
}
